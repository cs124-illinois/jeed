{
  "name": "@cs124/jeed-react",
  "version": "2025.12.4",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "check": "bun run depcheck && bun run prettier && bun run eslint && bun run tsc && bun run build",
    "clean": "rimraf dist",
    "depcheck": "depcheck --ignores=prettier-plugin-*,rimraf",
    "eslint": "eslint src/**",
    "ncu": "ncu --dep dev,prod",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "prepublish": "rimraf dist && tsc",
    "start": "tsc -w",
    "tsc": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@cs124/jeed-types": "2025.12.4"
  },
  "peerDependencies": {
    "react": ">= 16"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/react": "19.2.7",
    "depcheck": "1.4.7",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "npm-check-updates": "19.2.0",
    "prettier": "3.7.4",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "react": "19.2.3",
    "rimraf": "6.1.2",
    "runtypes": "6.7.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0"
  }
}
