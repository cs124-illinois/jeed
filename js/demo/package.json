{
  "private": true,
  "name": "demo",
  "version": "2025.3.0",
  "scripts": {
    "build": "next build",
    "checker": "npm run eslint && npm run tsc && npm run format && npm run depcheck",
    "depcheck": "depcheck --ignore-dirs .next,.rush,bundled --ignores=@types/*,prettier-plugin-*",
    "deploy": "gh-pages -t -d dist",
    "eslint": "eslint --fix pages --max-warnings=0 --ext .ts,.tsx",
    "export": "rimraf dist && NEXT_PUBLIC_BASE_PATH=/jeed yarn build && touch dist/.nojekyll",
    "format": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@cs124/jeed-output": "2025.3.0",
    "@cs124/jeed-react": "2025.3.0",
    "@cs124/jeed-types": "2025.3.0",
    "next": "15.2.2",
    "react": "19.0.0",
    "react-ace": "14.0.1",
    "react-dom": "19.0.0",
    "runtypes": "6.7.0"
  },
  "devDependencies": {
    "@types/react": "19.0.10",
    "depcheck": "1.4.7",
    "eslint": "8.57.0",
    "eslint-config-next": "15.2.2",
    "gh-pages": "6.3.0",
    "prettier": "3.5.3",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "4.1.0",
    "sass": "1.85.1",
    "typescript": "5.8.2"
  }
}
