{
  "private": true,
  "name": "demo",
  "version": "2025.12.4",
  "scripts": {
    "build": "next build",
    "check": "bun run eslint && bun run tsc && bun run format && bun run depcheck",
    "depcheck": "depcheck --ignore-dirs .next,.rush,bundled --ignores=@types/*,prettier-plugin-*,rimraf",
    "deploy": "gh-pages -t -d dist",
    "eslint": "eslint --fix pages --max-warnings=0 --ext .ts,.tsx",
    "export": "rimraf dist && NEXT_PUBLIC_BASE_PATH=/jeed bun run build && touch dist/.nojekyll",
    "format": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "lint": "next lint",
    "ncu": "ncu",
    "serve": "next start",
    "start": "next dev",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@cs124/jeed-output": "2025.12.4",
    "@cs124/jeed-react": "2025.12.4",
    "@cs124/jeed-types": "2025.12.4",
    "next": "15.5.6",
    "react": "19.2.3",
    "react-ace": "14.0.1",
    "react-dom": "19.2.3",
    "runtypes": "6.7.0"
  },
  "devDependencies": {
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "depcheck": "1.4.7",
    "eslint": "8.57.0",
    "eslint-config-next": "15.5.6",
    "gh-pages": "6.3.0",
    "npm-check-updates": "19.2.0",
    "prettier": "3.7.4",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "rimraf": "6.1.2",
    "sass": "1.97.1",
    "typescript": "5.9.3"
  }
}
