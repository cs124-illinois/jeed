{
  "private": true,
  "name": "demo",
  "version": "2024.5.0",
  "scripts": {
    "build": "next build",
    "checker": "npm run eslint && npm run tsc && npm run format && npm run depcheck",
    "depcheck": "depcheck --ignore-dirs .next,.rush,bundled --ignores=@types/*,prettier-plugin-*",
    "deploy": "gh-pages -t -d dist",
    "eslint": "eslint --fix pages --max-warnings=0 --ext .ts,.tsx",
    "export": "rimraf dist && NEXT_PUBLIC_BASE_PATH=/jeed yarn build && touch dist/.nojekyll",
    "format": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@cs124/jeed-output": "2024.5.0",
    "@cs124/jeed-react": "2024.5.0",
    "@cs124/jeed-types": "2024.5.0",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-ace": "11.0.1",
    "react-dom": "18.3.1",
    "runtypes": "6.7.0"
  },
  "devDependencies": {
    "@types/react": "18.3.3",
    "depcheck": "1.4.7",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "gh-pages": "6.1.1",
    "prettier": "3.2.5",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "sass": "1.77.3",
    "typescript": "5.4.5"
  }
}
