{
  "name": "@cs124/jeed-output",
  "version": "2024.5.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "checker": "yarn depcheck && yarn prettier && yarn eslint && yarn tsc && yarn build",
    "clean": "rimraf dist",
    "depcheck": "depcheck --ignores=prettier-plugin-*,rimraf",
    "eslint": "eslint src/**",
    "ncu": "ncu",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "prepublish": "rimraf dist && tsc",
    "start": "yarn watch",
    "test": "JEED_SERVER=http://localhost:8888 ts-mocha --paths tests/index.spec.ts",
    "watch": "tsc -w"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@cs124/jeed-types": "2024.5.0"
  },
  "devDependencies": {
    "@eslint/js": "9.3.0",
    "depcheck": "1.4.7",
    "eslint": "9.3.0",
    "eslint-config-prettier": "9.1.0",
    "npm-check-updates": "16.14.20",
    "prettier": "3.2.5",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "rimraf": "5.0.7",
    "runtypes": "6.7.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.11.0"
  }
}
